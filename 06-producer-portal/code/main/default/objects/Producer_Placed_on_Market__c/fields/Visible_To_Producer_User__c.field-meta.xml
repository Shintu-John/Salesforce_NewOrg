<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Visible_To_Producer_User__c</fullName>
    <description>ADDMONTHS(Quarter_Start_Date__c, 3) &lt;= TODAY() &amp;&amp;</description>
    <formula>Quarter_Start_Date__c &lt;= TODAY() &amp;&amp;
OR(
(RecordType.Name == &quot;Household&quot; &amp;&amp; INCLUDES(Account__r.Producer_Contract_Obligation_Types__c, &quot;Household&quot;)),
(RecordType.Name == &quot;Non-Household&quot; &amp;&amp; INCLUDES(Account__r.Producer_Contract_Obligation_Types__c, 
&quot;Non-Household&quot;)),
(RecordType.Name == &quot;Household &amp; Non-Household&quot; &amp;&amp; INCLUDES(Account__r.Producer_Contract_Obligation_Types__c, &quot;Household &amp; Non-Household&quot;))
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Visible To Producer User</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
