<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>62.0</apiVersion>
    <areMetricsLoggedToDataCloud>false</areMetricsLoggedToDataCloud>
    <decisions>
        <name>Record_Type_Decision</name>
        <label>Record Type Decision</label>
        <locationX>270</locationX>
        <locationY>431</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Record_Type_Household_Household_Non_Household</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>$Record.RecordType.Name</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Household</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.RecordType.Name</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Household &amp; Non-Household</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Household_Related_Data</targetReference>
            </connector>
            <label>Record Type = Household, Household &amp; Non-Household</label>
        </rules>
        <rules>
            <name>Record_Type_Non_Household</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.RecordType.Name</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Non-household</stringValue>
                </rightValue>
            </conditions>
            <label>Record Type = Non-Household</label>
        </rules>
    </decisions>
    <decisions>
        <name>Update_Related_Producer_Placed_on_Market_Decision</name>
        <label>Update Related Producer Placed on Market Decision</label>
        <locationX>468</locationX>
        <locationY>323</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Update_Related_Producer_Placed_on_Market_Outcome</name>
            <conditionLogic>1 OR ((2 OR 4) AND 5) OR ((3 OR 4) AND 6)</conditionLogic>
            <conditions>
                <leftValueReference>ISNEW</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.RecordType.Name</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Household</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.RecordType.Name</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Non-Household</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.RecordType.Name</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Household &amp; Non-Household</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>IsHouseholdDataChanged</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>IsNonHouseholdDataChanged</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Record_Type_Decision</targetReference>
            </connector>
            <label>Update Related Producer Placed on Market Outcome</label>
        </rules>
    </decisions>
    <environments>Default</environments>
    <formulas>
        <name>IsHouseholdDataChanged</name>
        <dataType>Boolean</dataType>
        <expression>OR(
    ISCHANGED({!$Record.Category_1_Household__c}),
    ISCHANGED({!$Record.Category_2_Household__c}),
    ISCHANGED({!$Record.Category_3_Household__c}),
    ISCHANGED({!$Record.Category_4_Household__c}),
    ISCHANGED({!$Record.Category_5_Household__c}),
    ISCHANGED({!$Record.Category_6_Household__c}),
    ISCHANGED({!$Record.Category_7_Household__c}),
    ISCHANGED({!$Record.Category_8_Household__c}),
    ISCHANGED({!$Record.Category_9_Household__c}),
    ISCHANGED({!$Record.Category_10_Household__c}),
    ISCHANGED({!$Record.Category_11_Household__c}),
    ISCHANGED({!$Record.Category_12_Household__c}),
    ISCHANGED({!$Record.Category_13_Household__c}),
    ISCHANGED({!$Record.Category_14_Household__c})
)</expression>
    </formulas>
    <formulas>
        <name>ISNEW</name>
        <dataType>Boolean</dataType>
        <expression>ISNEW()</expression>
    </formulas>
    <formulas>
        <name>IsNonHouseholdDataChanged</name>
        <dataType>Boolean</dataType>
        <expression>OR(
    ISCHANGED({!$Record.Category_1_Non_Household__c}),
    ISCHANGED({!$Record.Category_2_Non_Household__c}),
    ISCHANGED({!$Record.Category_3_Non_Household__c}),
    ISCHANGED({!$Record.Category_4_Non_Household__c}),
    ISCHANGED({!$Record.Category_5_Non_Household__c}),
    ISCHANGED({!$Record.Category_6_Non_Household__c}),
    ISCHANGED({!$Record.Category_7_Non_Household__c}),
    ISCHANGED({!$Record.Category_8_Non_Household__c}),
    ISCHANGED({!$Record.Category_9_Non_Household__c}),
    ISCHANGED({!$Record.Category_10_Non_Household__c}),
    ISCHANGED({!$Record.Category_11_Non_Household__c}),
    ISCHANGED({!$Record.Category_12_Non_Household__c}),
    ISCHANGED({!$Record.Category_13_Non_Household__c}),
    ISCHANGED({!$Record.Category_14_Non_Household__c})
)</expression>
    </formulas>
    <interviewLabel>Update Related Producer Placed on Market {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Update Related Producer Placed on Market</label>
    <processMetadataValues>
