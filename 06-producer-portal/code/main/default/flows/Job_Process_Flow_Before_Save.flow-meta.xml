                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Lock_Job_Assignment</targetReference>
            </connector>
            <label>TRUE_Job Status change to Cancelled?</label>
        </rules>
    </decisions>
    <decisions>
        <name>myRule_4</name>
        <label>Paperwork Uploaded</label>
        <locationX>578</locationX>
        <locationY>431</locationY>
        <defaultConnectorLabel>FALSE_Paperwork Uploaded</defaultConnectorLabel>
        <rules>
            <name>TRUE_myRule_4</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Status__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Completed</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Required_Paperwork__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Status__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Cancelled</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Status__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Failed</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>isPaperworkDone</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Status_to_Paperwork_Provided</targetReference>
            </connector>
            <label>TRUE_Paperwork Uploaded</label>
        </rules>
        <rules>
            <name>Downgrade_Status_from_Paperwork_Provided</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Paperwork Provided</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>isPaperworkDonePrior</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Status_to_Collected</targetReference>
            </connector>
            <label>Downgrade Status from Paperwork Provided</label>
        </rules>
    </decisions>
