<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>59.0</apiVersion>
    <areMetricsLoggedToDataCloud>false</areMetricsLoggedToDataCloud>
    <environments>Default</environments>
    <interviewLabel>Specialist Support: Daily Log Update Last Contact {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Specialist Support: Daily Log Update Last Contact</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>AUTO_LAYOUT_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <recordUpdates>
        <name>UpdateLast</name>
        <label>UpdateLastContacted</label>
        <locationX>176</locationX>
        <locationY>323</locationY>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Id</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>$Record.Specialist_Support_Client__r.Id</elementReference>
            </value>
        </filters>
        <inputAssignments>
            <field>Last_Contacted__c</field>
            <value>
                <elementReference>$Record.Date_of_Log__c</elementReference>
            </value>
        </inputAssignments>
        <object>Specialist_Support_Client__c</object>
    </recordUpdates>
    <start>
        <locationX>50</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>UpdateLast</targetReference>
        </connector>
        <filterFormula>(ISNEW()||
ISCHANGED({!$Record.Date_of_Log__c})||
ISCHANGED({!$Record.Specialist_Support_Client__c}))
&amp;&amp;
NOT(ISBLANK({!$Record.Specialist_Support_Client__c}))</filterFormula>
        <object>Daily_Log__c</object>
        <recordTriggerType>CreateAndUpdate</recordTriggerType>
        <triggerType>RecordAfterSave</triggerType>
    </start>
    <status>Draft</status>
</Flow>
