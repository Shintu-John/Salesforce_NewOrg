    <decisions>
        <name>File_Import_Status_Decision</name>
        <label>File Import Status Decision</label>
        <locationX>176</locationX>
        <locationY>278</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>File_Imported_Successfully</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_File_Import.File_Import_Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Completed</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_File_Import.No_Of_Mismatches__c</leftValueReference>
                <operator>LessThanOrEqualTo</operator>
                <rightValue>
                    <numberValue>0.0</numberValue>
                </rightValue>
            </conditions>
            <label>File Imported Successfully</label>
        </rules>
        <rules>
            <name>Create_Case_outcome</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_File_Import.No_Of_Mismatches__c</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <numberValue>0.0</numberValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_File_Import.File_Import_Status__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Completed</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Paperwork_Compliance_RecordType</targetReference>
            </connector>
            <label>Create Case outcome</label>
        </rules>
    </decisions>
    <description>- remove team assign to supply chain
- attach file import documents to case
- no of mismatches should be greater than 0 to create the case
-supplier from file import added on case subject</description>
    <environments>Default</environments>
    <formulas>
        <name>caseSubject</name>
        <dataType>String</dataType>
        <expression>{!Get_File_Import.Processed_Document__r.Name} + &quot; &quot; +TEXT({!Get_File_Import.No_Of_Mismatches__c}) + &quot; error(s) against &quot; + {!Get_File_Import.Tag__c} + &quot; from &quot; + {!Get_File_Import.Supplier__c}</expression>
    </formulas>
    <interviewLabel>File Import Failed Case Creation {!$Flow.CurrentDateTime}</interviewLabel>
    <label>File Import Failed Case Creation</label>
