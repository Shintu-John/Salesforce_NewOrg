<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>63.0</apiVersion>
    <areMetricsLoggedToDataCloud>false</areMetricsLoggedToDataCloud>
    <description>Migrated from the Resident evicted process using Does picklist equal Evicted? criteria.
When resident evicted through daily log, update resident profile</description>
    <label>Resident evicted_1</label>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>AUTO_LAYOUT_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <recordUpdates>
        <name>myRule_1_A1</name>
        <label>Amend resident profile status</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <inputAssignments>
            <field>Reason_for_status__c</field>
            <value>
                <stringValue>Evicted</stringValue>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>Status_of_application__c</field>
            <value>
                <stringValue>Evicted</stringValue>
            </value>
        </inputAssignments>
        <inputReference>$Record.Resident__r</inputReference>
    </recordUpdates>
    <start>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>myRule_1_A1</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Type_of_Log__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>Eviction</stringValue>
            </value>
        </filters>
        <object>Daily_Log__c</object>
        <recordTriggerType>CreateAndUpdate</recordTriggerType>
        <triggerType>RecordAfterSave</triggerType>
    </start>
    <status>Draft</status>
</Flow>
