    <description>02-02-2023 - populate compliance email
        default country &quot;United Kingdom&quot; for supplier account</description>
    <environments>Default</environments>
    <formulas>
        <name>billingCountryInitialValue</name>
        <dataType>String</dataType>
        <expression>IF( and({!$Record.BillingCountry} == null, {!$Record.RecordType.DeveloperName}
            == &quot;Supplier&quot;) ,&quot;United Kingdom&quot;,{!$Record.BillingCountry})</expression>
    </formulas>
    <formulas>
        <name>fullName</name>
        <dataType>String</dataType>
        <expression>IF(
            NOT(ISNULL({!$Record.Compliance_Contact__c})),
            IF(NOT(ISBLANK({!$Record.Compliance_Contact__r.FirstName})),{!$Record.Compliance_Contact__r.FirstName}+&quot; &quot;+{!$Record.Compliance_Contact__r.LastName},{!$Record.Compliance_Contact__r.LastName}),&quot;&quot;)</expression>
    </formulas>
